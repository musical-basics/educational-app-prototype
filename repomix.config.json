{
    "output": {
        "filePath": "repomix-output.xml",
        "style": "xml"
    },
    "ignore": {
        "useGitignore": true,
        "useDefaultPatterns": true,
        "customPatterns": [
            "node_modules",
            ".next",
            "pnpm-lock.yaml",
            "components/ui/**",
            "public/**",
            ".DS_Store",
            "*.png",
            "*.jpg",
            "*.svg",
            "repomix-output.*",
            "repomix.config.json",
            "*.xml"
        ]
    }
}